{
  "name": "mybricks-webview",
  "displayName": "MyBricks.ai",
  "description": "MyBricks.ai 可视化开发平台 - VSCode 集成版，支持小程序、H5 页面可视化搭建",
  "version": "0.1.5",
  "publisher": "mybricks",
  "repository": {
    "type": "git",
    "url": "https://github.com/mybricks/mybricks-ide-as-vscode-plugin.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mybricks.openIDE",
        "title": "Open MyBricks IDE",
        "icon": "src/extension/assets/icon.svg"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "mybricks-sidebar",
          "title": "低代码",
          "icon": "src/extension/assets/icon.svg"
        }
      ]
    },
    "views": {
      "mybricks-sidebar": [
        {
          "type": "webview",
          "id": "mybricks.ide",
          "name": "低代码",
          "visibility": "hidden"
        }
      ]
    },
    "configuration": {
      "title": "MyBricks",
      "properties": {}
    }
  },
  "dependencies": {
    "@mybricks/to-code-taro": "^1.1.8"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@mybricks/plugin-connector-http": "^1.2.37",
    "@mybricks/rxui": "^1.0.87",
    "@mybricks/sdk-for-app": "1.0.69",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/vscode": "^1.60.0",
    "antd": "^5.22.7",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.3",
    "dayjs": "^1.11.13",
    "html-webpack-plugin": "^5.6.6",
    "idb-keyval": "^6.2.1",
    "less": "^4.2.0",
    "less-loader": "^12.3.0",
    "mini-css-extract-plugin": "^2.10.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxui-t": "0.0.5",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "vite": "^5.1.0",
    "webpack": "^5.105.0",
    "webpack-cli": "^6.0.1",
    "@mybricks/plugin-ai": "^0.0.4",
    "axios": "^1.13.2"
  },
  "scripts": {
    "build:webview": "webpack --mode production",
    "watch:webview": "webpack --mode development --watch",
    "build:vsix": "node scripts/ensure-unpackage.js && npm run build:webview && vsce package --out ./unpackage"
  }
}
