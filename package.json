{
  "name": "mybricks-webview",
  "displayName": "MyBricks.ai",
  "description": "MyBricks.ai 可视化开发平台 - VSCode 集成版，支持小程序、H5 页面可视化搭建",
  "version": "0.0.13",
  "publisher": "mybricks",
  "repository": {
    "type": "git",
    "url": "https://github.com/mybricks/mybricks-ide-as-vscode-plugin.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mybricks.openIDE",
        "title": "Open MyBricks IDE",
        "icon": "asserts/icon.svg"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "mybricks-sidebar",
          "title": "低代码",
          "icon": "asserts/icon.svg"
        }
      ]
    },
    "views": {
      "mybricks-sidebar": [
        {
          "type": "webview",
          "id": "mybricks.ide",
          "name": "低代码",
          "visibility": "hidden"
        }
      ]
    },
    "configuration": {
      "title": "MyBricks",
      "properties": {}
    }
  },
  "dependencies": {
    "@mybricks/plugin-connector-http": "^1.2.36",
    "@mybricks/to-code-taro": "^1.1.5"
  }
}
